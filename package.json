{
  "name": "saber-vdom",
  "version": "0.1.5",
  "description": "a fast vdom renderer for javascript.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Saber2pr/saber-vdom.git"
  },
  "author": "saber2pr",
  "license": "ISC",
  "files": [
    "lib/core/*",
    "lib/index.*"
  ],
  "main": "./lib/index.js",
  "scripts": {
    "test": "node ./lib/test/test.js",
    "start": "tsc --watch",
    "dev": "rollup -c --watch",
    "min": "uglifyjs ./build/bundle.js -o saber-vdom.min.js -c -m",
    "build": "rm -r ./lib && tsc && rollup -c && npm run min"
  },
  "keywords": [
    "vdom",
    "typescript",
    "fiber"
  ],
  "devDependencies": {
    "rollup": "^1.1.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "typescript": "^3.2.1",
    "uglify-js": "^3.4.9"
  },
  "dependencies": {
    "htm": "^2.1.1"
  }
}